C,CAAC,CAAC;YACT,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACV,CAAC,CAAC,CAAC,CAAC,CAAC;IACT,CAAC,CAAC,CAAC,CAAC,CAAC","file":"C:/Users/justi/PycharmProjects/professor's assignment 3/subscriptions-app/src/components/Edit.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <div class=\"row justify-content-center\">\r\n        <div class=\"col-md-5\">\r\n            <h3 class=\"text-center\">Update Game</h3>\r\n            <form @submit.prevent=\"onUpdateForm\">\r\n                <div class=\"form-group\">\r\n                    <label>Name</label>\r\n                    <input type=\"text\" class=\"form-control\" v-model=\"game.name\" required>\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                    <label>Description</label>\r\n                    <input type=\"text\" class=\"form-control\" v-model=\"game.description\" required>\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                    <label>Console</label>\r\n                    <input type=\"text\" class=\"form-control\" v-model=\"game.console\" required>\r\n                </div>.\r\n\r\n                <div class=\"form-group\">\r\n                    <label>Amount</label>\r\n                    <input type=\"text\" class=\"form-control\" v-model=\"game.amount\" required>\r\n                </div>\r\n\r\n              <div class=\"form-group\">\r\n                    <label>Format</label>\r\n                    <select id='frequency' class=\"form-control\" v-model=\"game.digital\">\r\n                      <option value='physical'>Physical</option>\r\n                      <option value='digital'>Digital</option>\r\n                    </select>\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                    <button class=\"btn btn-primary btn-block\">Update Game</button>\r\n                </div>\r\n            </form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport db from '../firebaseDb'\r\n\r\nexport default {\r\n  name: 'Edit',\r\n  data () {\r\n    return {\r\n      game: {\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    let dbRef = db.collection('game').doc(this.$route.params.id)\r\n    dbRef.get().then((doc) => {\r\n      this.game = doc.data()\r\n    }).catch((error) => {\r\n      console.log(error)\r\n    })\r\n  },\r\n  methods: {\r\n    onUpdateForm (event) {\r\n      event.preventDefault()\r\n      db.collection('games').doc(this.$route.params.id)\r\n        .update(this.game).then(() => {\r\n          console.log('Game successfully updated!')\r\n          this.$router.push('/')\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}