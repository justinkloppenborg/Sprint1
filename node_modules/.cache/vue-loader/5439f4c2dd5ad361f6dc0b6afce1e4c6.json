bimonthly'>Bi-monthly</option>\r\n                      <option value='monthly'>Monthly</option>\r\n                      <option value='quarterly'>Quarterly</option>\r\n                      <option value='biannually'>Bi-annually</option>\r\n                      <option value='annually'>Annually</option>\r\n                    </select>\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                    <button class=\"btn btn-primary btn-block\">Update Subscription</button>\r\n                </div>\r\n            </form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport db from '../firebaseDb'\r\n\r\nexport default {\r\n  name: 'Edit',\r\n  data () {\r\n    return {\r\n      subscription: {\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    let dbRef = db.collection('subscriptions').doc(this.$route.params.id)\r\n    dbRef.get().then((doc) => {\r\n      this.subscription = doc.data()\r\n    }).catch((error) => {\r\n      console.log(error)\r\n    })\r\n  },\r\n  methods: {\r\n    onUpdateForm (event) {\r\n      event.preventDefault()\r\n      db.collection('subscriptions').doc(this.$route.params.id)\r\n        .update(this.subscription).then(() => {\r\n          console.log('Subscription successfully updated!')\r\n          this.$router.push('/')\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n"]}]}